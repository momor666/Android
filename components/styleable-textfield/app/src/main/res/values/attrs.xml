<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="fontStyle" format="enum">
        <enum name="roboto_regular" value="0"/>
        <enum name="roboto_italic" value="1"/>
        <enum name="roboto_light" value="2"/>
        <enum name="roboto_light_italic" value="3"/>
        <enum name="roboto_thin" value="4"/>
        <enum name="roboto_thin_italic" value="5"/>
        <enum name="roboto_medium" value="6"/>
        <enum name="roboto_medium_italic" value="7"/>
        <enum name="roboto_bold" value="8"/>
        <enum name="roboto_bold_italic" value="9"/>
        <enum name="roboto_black" value="10"/>
        <enum name="roboto_black_italic" value="11"/>
        <enum name="roboto_condensed_regular" value="12"/>
        <enum name="roboto_condensed_italic" value="13"/>
        <enum name="roboto_condensed_light" value="14"/>
        <enum name="roboto_condensed_light_italic" value="15"/>
        <enum name="roboto_condensed_bold" value="16"/>
        <enum name="roboto_condensed_bold_italic" value="17"/>
    </attr>

    <declare-styleable name="CustomTextView">
        <attr name="fontStyle"/>
    </declare-styleable>
</resources>